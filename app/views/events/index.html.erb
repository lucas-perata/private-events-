<p style="color: green"><%= notice %></p>

<h1 class="font-medium leading-tight text-5xl mt-0 mb-2 text-blue-600">Events</h1>

<div id="events">
  <% @events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event %>
    </p>
  <% end %>
</div>

<%= link_to "New event", new_event_path %>
